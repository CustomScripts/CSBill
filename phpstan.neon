includes:
    - vendor/phpstan/phpstan-doctrine/extension.neon
    - vendor/phpstan/phpstan-symfony/extension.neon

parameters:
    bootstrap: %rootDir%/../../../app/autoload.php
    autoload_files:
        - vendor/bin/.phpunit/phpunit-7.5-0/vendor/autoload.php
    excludes_analyse:
        - src/CoreBundle/Tests/Fixtures/file.php
    symfony:
        container_xml_path: %rootDir%/../../../var/cache/test/appAppKernelTestDebugContainer.xml
