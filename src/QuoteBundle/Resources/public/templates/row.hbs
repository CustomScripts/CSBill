{{#each fields}}
    <td class="column-{{@key}}">
        {{{replace this '__name__' ../id}}}
    </td>
{{/each}}
<td class="column-total">
    {{currency 0}}
</td>
<td class="column-trash text-center">
    <a href="#" class="remove-item" rel="tooltip" title="{{trans "quote.item.remove" }}">
        <i class="fa fa-trash-o"></i>
    </a>
</td>